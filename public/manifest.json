{
  "api": [
    {
      "name": "query",
      "url": "https://search-engine.wuran.link/api/v1",
      "description": "查询搜索引擎获取信息",
      "parameters": {
        "properties": {
          "query": {
            "description": "搜索的文本内容",
            "type": "string"
          }
        },
        "required": ["query"],
        "type": "object"
      }
    }
  ],
  "identifier": "search-engine",
  "settings": {
    "type": "object",
    "required": ["SERPAPI_API_KEY"],
    "properties": {
      "SERPAPI_API_KEY": {
        "title": "SerpAPI API Key",
        "description": "we use SerpAPI as backend service [Learn more](https://github.com/lobehub/chat-plugin-search-engine) | 该插件使用 SerpAPI 作为搜索服务",
        "type": "string",
        "minLength": 64,
        "maxLength": 64,
        "format": "password"
      }
    }
  },
  "ui": {
    "url": "https://search-engine.wuran.link/dist/index.js",
    "mode": "module"
  },
  "version": "1"
}
